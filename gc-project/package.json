{
  "name": "gc-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ssl": "concurrently \"next dev\" \"local-ssl-proxy --source 3010 --target 3000 --cert localhost+2.pem --key localhost+2-key.pem\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "storyblok:pull": "storyblok components pull --space 289122267088853",
    "storyblok:generate": "storyblok types generate --space 289122267088853",
    "storyblok:copy": "cpx \".storyblok/types/289122267088853/*.d.ts\" src/types/auto-generated/ && cpx \".storyblok/types/storyblok.d.ts\" src/types/",
    "storyblok:types": "npm run storyblok:pull && npm run storyblok:generate && npm run storyblok:copy",
    "free-port": "lsof -ti :3000 | xargs kill -9 || true && lsof -ti :3010 | xargs kill -9 || true"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@react-three/postprocessing": "^3.0.4",
    "@react-three/rapier": "^2.2.0",
    "@storyblok/react": "^5.4.20",
    "classnames": "^2.5.1",
    "next": "16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "storyblok-js-client": "^7.2.1",
    "storyblok-rich-text-react-renderer": "^3.0.1",
    "three": "^0.182.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.178.0",
    "concurrently": "^9.2.1",
    "cpx": "^1.5.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "local-ssl-proxy": "^2.0.5",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
